{
    "name": "biswajit/lifesteal",
    "description": "A lifesteal plugin for your server.",
    "type": "project",
    "require": {
        "customiesdevs/customies": "dev-main",
		"pocketmine/pocketmine-mp": "^5.0.0"
	},
	"require-dev": {
		"phpstan/phpstan": "^1.2.0",
		"phpstan/phpstan-strict-rules": "^1.0",
		"phpstan/extension-installer": "^1.0"
	},
	"autoload": {
		"psr-4": {
			"": "src/"
		}
	},
	"config": {
		"allow-plugins": {
			"phpstan/extension-installer": true
		}
	}
}